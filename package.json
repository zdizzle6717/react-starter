{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A straight forward starter kit for latest versions of react and redux with custom library",
  "main": "src/index.jsx",
  "repository": "https://github.com/zdizzle6717/react-starter.git",
  "author": "Zachary Anselm <zachary.anselm@egov.com>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-client": "babel-watch server-client/index.jsx",
    "start-client": "forever start -c 'node_modules/.bin/babel-node' server-client/index.jsx",
    "stop-client": "forever stop 'server-client/index.js'",
    "watch-server": "babel-watch server/index.js",
    "start-server": "babel server --out-dir compiled-server && pm2 start compiled-server/index.js --name='Sandbox3API'",
    "stop-server": "pm2 stop 'Sandbox3API'",
    "build": "node build-style.js && NODE_ENV=development node_modules/.bin/webpack -d --config webpack.dev.js",
    "build-prod": "node build-style.js && NODE_ENV=production node_modules/.bin/webpack -p --config webpack.prod.js",
    "deploy": "npm run build-prod && npm run start-server && npm run start-client",
    "style": "node build-style.js"
  },
  "dependencies": {
    "attr-accept": "^1.1.0",
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bad-words": "^1.5.1",
    "bcrypt": "^1.0.2",
    "boom": "^4.3.1",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "foundation": "^4.2.1-1",
    "foundation-sites": "^6.3.1",
    "fs-extra": "^2.1.2",
    "gm": "^1.23.0",
    "hapi": "^16.1.1",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-swagger": "^7.7.0",
    "hbs": "^4.0.1",
    "history": "^4.6.1",
    "hoek": "^4.1.1",
    "inert": "^4.2.0",
    "joi": "^10.4.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.0",
    "keymirror": "^0.1.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "nodemailer": "^4.0.1",
    "nouislider": "^9.2.0",
